{
	"manifest_version": 2,
	"name": "Dark Kakuge-Checker",
	"version": "1.2",
	"icons": {
		"16": "icon16.png",
		"128": "icon128.png"
	},
	"description": "A Dark theme for https://kakuge-checker.com",
	"content_scripts": [
		{
			"matches": ["https://kakuge-checker.com/*"],
			"css": ["app.css"],
			"run_at": "document_end"	
		}
	]
}